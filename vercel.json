{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],


  "rewrites": [
    
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"

    },
    {
      "source": "/(.*)",
      "destination": "/index.html"

    }
    
  ]
}